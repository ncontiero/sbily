{
  "name": "sbily",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.18.2",
  "description": "A Django project for a link shortener.",
  "author": "Nicolas Contiero <https://github.com/ncontiero>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production rspack build -c rspack/prod.config.ts",
    "build:email": "pnpm --filter=@sbily/emails run export",
    "dev": "cross-env NODE_ENV=development rspack serve -c rspack/dev.config.ts",
    "dev:email": "pnpm --filter=@sbily/emails run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@stripe/stripe-js": "^8.0.0",
    "chart.js": "^4.5.1",
    "chartjs-chart-geo": "^4.3.6",
    "lucide": "^0.545.0"
  },
  "devDependencies": {
    "@ncontiero/eslint-config": "^6.3.1",
    "@rspack/cli": "^1.5.8",
    "@rspack/core": "^1.5.8",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^24.7.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.4.0",
    "tailwindcss": "3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack-bundle-tracker": "^3.2.1",
    "webpack-merge": "^6.0.1"
  },
  "engines": {
    "node": ">=20"
  }
}
